<h1>Hola!! Esta es la página de inicio de mi aplicación.</h1>
<h2>Lista de Productos:</h2>
<div>
    {{#each products}}
    <div>
        <h3>{{title}}</h3>
        <p>{{description}}</p>
        <ul>
            <li>Precio: ${{price}}</li>
            <li>Stock: {{stock}}</li>
            <li>Categoría: {{category}}</li>
        </ul>
    </div>
    {{/each}}
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/home.js"></script>